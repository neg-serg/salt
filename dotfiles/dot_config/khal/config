[calendars]
[[calendars_discovery]]
path = ~/.config/vdirsyncer/calendars/
type = discover

[locale]
timeformat = %H:%M
dateformat = %d.%m.%Y
longdateformat = %d.%m.%Y %H:%M
datetimeformat = %d.%m.%Y %H:%M
longdatetimeformat = %d.%m.%Y %H:%M
firstweekday = 0
default_timezone = Europe/Moscow

[default]
show_all_days = False
