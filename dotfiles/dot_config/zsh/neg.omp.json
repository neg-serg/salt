{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
        {
            "alignment": "right",
            "overflow": "hide",
            "segments": [
                {
                    "type": "git",
                    "style": "plain",
                    "properties": {
                        "branch_icon": "",
                        "fetch_status": true
                    },
                    "template": "<#005faf>⟮</><#6C7E96>{{ if .Data.HEAD }}<b>{{ .Data.HEAD }}</b>{{ end }}{{ if gt .Data.Ahead 0 }}<#367CB0>←{{ .Data.Ahead }}</>{{ end }}{{ if gt .Data.Behind 0 }}<#367CB0>→{{ .Data.Behind }}</>{{ end }}{{ if and .Data.Working (gt .Data.Working.Changed 0) }} <#914E89>●W{{ .Data.Working.String }}</>{{ end }}{{ if and .Data.Staging (gt .Data.Staging.Changed 0) }} <#5E468C>●S{{ .Data.Staging.String }}</>{{ end }}<#005faf>⟯</>"
                },
                {
                    "type": "executiontime",
                    "style": "plain",
                    "foreground": "#45bf17",
                    "template": " {{ .FormattedMs }}s <#899CA1>\ue601</>",
                    "properties": {
                        "threshold": 50,
                        "style": "dallas"
                    }
                },
                {
                    "type": "npm",
                    "style": "plain",
                    "foreground": "#ce162b",
                    "template": "<#899CA1>and</> \ue71e {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} "
                },
                {
                    "foreground": "#E4F34A",
                    "properties": {},
                    "template": "\ue235{{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}",
                    "type": "python",
                    "style": "plain"
                },
                {
                    "foreground": "#7FD5EA",
                    "properties": {},
                    "template": "\ue626{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
                    "type": "go",
                    "style": "plain"
                },
                {
                    "foreground": "#42E66C",
                    "properties": {
                        "detect_extensions": [
                            "js",
                            "ts"
                        ],
                        "fetch_version": true,
                        "cache_duration": "3m"
                    },
                    "template": " \ue718{{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
                    "type": "node",
                    "style": "plain"
                },
                {
                    "foreground": "#CC342D",
                    "properties": {},
                    "template": "\ue791{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
                    "type": "ruby",
                    "style": "plain"
                },
                {
                    "foreground": "#b07219",
                    "properties": {},
                    "template": "\ue738{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
                    "type": "java",
                    "style": "plain"
                },
                {
                    "foreground": "#0db7ed",
                    "properties": {},
                    "template": "\uf308{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
                    "type": "docker",
                    "style": "plain"
                },
                {
                    "foreground": "#5f43e9",
                    "properties": {},
                    "template": "TF{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
                    "type": "terraform",
                    "style": "plain"
                },
                {
                    "foreground": "#f7ad24",
                    "properties": {},
                    "template": "\ue718{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
                    "type": "pnpm",
                    "style": "plain"
                },
                {
                    "type": "status",
                    "style": "plain",
                    "foreground_templates": [
                        "{{ if gt .Code 0 }}#ce162b{{ end }}"
                    ],
                    "properties": {
                        "always_enabled": false
                    },
                    "template": " ×{{.Code}}"
                }
            ],
            "type": "rprompt"
        },
        {
            "alignment": "left",
            "segments": [
                {
                    "foreground": "#005faf",
                    "properties": {
                        "linux": "<#005faf>\ue27f</>",
                        "macos": "<#005faf>\ue27f</>",
                        "windows": "<#005faf>\ue27f</>"
                    },
                    "style": "plain",
                    "template": "{{ if false }}{{ if .WSL }}WSL at {{ end }} {{.Icon}} {{ end }}",
                    "type": "os"
                },
                {
                    "type": "path",
                    "style": "plain",
                    "foreground": "#95a7bc",
                    "template": "<#005faf></> {{ if hasPrefix \"/\" .Path }}<#005faf>/</>{{ trimPrefix \"/\" .Path }}{{ else }}{{ .Path }}{{ end }}",
                    "properties": {
                        "style": "full",
                        "folder_separator_icon": "<#005faf>/</>",
                        "home_icon": "<#287373>~</>"
                    }
                },
                {
                    "foreground": "#ce162b",
                    "style": "plain",
                    "template": "\uf0e7 ",
                    "type": "root"
                },
                {
                    "type": "text",
                    "style": "plain",
                    "foreground": "#005faf",
                    "template": " ❯"
                }
            ],
            "type": "prompt"
        }
    ],
    "final_space": true,
    "version": 3
}